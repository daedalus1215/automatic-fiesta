{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "lib": [
      "ES2015",
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@shared/*": [
        "src/shared/*"
      ],
      "@components/*": [
        "src/shared/components/*"
      ],
      "@utils/*": [
        "src/shared/utils/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
    },
  },
  "include": [
    "vite.config.ts"
  ]
}